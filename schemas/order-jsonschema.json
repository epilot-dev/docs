            "payment_sepa",
            "payment_invoice",
            "payment_cash"
          ],
          "example": "payment_sepa"
        },
        "data": {
          "type": "object",
          "properties": {
            "bank_name": {
              "type": "string",
              "example": "Deutsche Bank"
            },
            "iban": {
