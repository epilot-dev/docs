{
  "name": "epilot-dev-handbook",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "dev": "docusaurus start",
    "start": "npm run fetch-changelogs && docusaurus start",
    "build": "npm run fetch-changelogs && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "eslint src --ext js,jsx,ts,tsx,json",
    "lint:fix": "eslint src --ext js,jsx,ts,tsx,json --fix",
    "pdf": "./scripts/build-pdf.sh",
    "fetch-changelogs": "./scripts/get-changelogs.sh",
    "update-entities": "cp ../entity-api/lambda/ApiHandlerFunction/src/schema/blueprints/snapshots/* static/schemas/",
    "update-events": "cp ../event-catalog-api/packages/core/src/builtin-events/__snapshots__/* static/events"
  },
  "dependencies": {
    "@cmfcmf/docusaurus-search-local": "^0.10.0",
    "@docusaurus/core": "2.0.0-beta.15",
    "@docusaurus/plugin-client-redirects": "^2.0.0-beta.15",
    "@docusaurus/plugin-content-docs": "2.0.0-beta.15",
    "@docusaurus/preset-classic": "2.0.0-beta.15",
    "@edno/docusaurus2-graphql-doc-generator": "^1.9.0",
    "@epilot360/icons": "^1.17.35",
    "@graphql-tools/url-loader": "^7.9.1",
    "@mdx-js/react": "^1.6.21",
    "@monaco-editor/react": "^4.7.0",
    "@stoplight/json-schema-viewer": "^4.16.4",
    "@stoplight/markdown-viewer": "^5.7.1",
    "@stoplight/mosaic": "^1.53.5",
    "@svgr/webpack": "^5.5.0",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "mdx-mermaid": "^1.3.2",
    "mermaid": "^11.12.2",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "redocusaurus": "^0.6.0",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-beta.15",
    "@epilot/eslint-config": "^1.2.10",
    "@tsconfig/docusaurus": "^1.0.4",
    "@types/react": "^17.0.14",
    "@types/react-helmet": "^6.1.2",
    "@types/react-router-dom": "^5.1.8",
    "eslint": "^7.15.0",
    "graphql": "^16.4.0",
    "postcss-prefix-selector": "^2.1.1",
    "typescript": "^4.3.5"
  },
  "overrides": {
    "redoc": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
